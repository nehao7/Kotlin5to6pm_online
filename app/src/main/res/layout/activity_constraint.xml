<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ConstraintActivity">

    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/h_gd_35"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".35"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/h_gd_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".40"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/h_gd_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".5"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/h_gd_6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".6"/>

    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/h_gd_65"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".65"/>

    <androidx.constraintlayout.widget.Guideline
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/v_gd_4"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".4"/>

    <androidx.constraintlayout.widget.Guideline
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/v_gd_50"
        app:layout_constraintGuide_percent=".50"
        android:orientation="vertical"/>

    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:id="@+id/v_gd_6"
        app:layout_constraintGuide_percent=".60"/>

    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/v_gd_20"
        app:layout_constraintGuide_percent=".20"
        android:orientation="vertical"/>

    <ImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/baseline_compare_arrows_24"
        android:elevation="10dp"
        app:layout_constraintTop_toTopOf="@id/h_gd_4"
        app:layout_constraintBottom_toBottomOf="@id/h_gd_6"
        app:layout_constraintStart_toStartOf="@id/v_gd_4"
        app:layout_constraintEnd_toEndOf="@id/v_gd_6"/>

    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        android:background="@color/green"
        android:layout_marginEnd="-30dp"
        android:text="@string/app_name"
        app:layout_constraintStart_toStartOf="@+id/v_gd_20"
        app:layout_constraintEnd_toStartOf="@id/v_gd_4"
        app:layout_constraintTop_toTopOf="@id/h_gd_35"
        app:layout_constraintBottom_toBottomOf="@id/h_gd_65"/>



</androidx.constraintlayout.widget.ConstraintLayout>